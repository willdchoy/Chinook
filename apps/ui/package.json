{
	"name": "chinook-ui",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "npx @biomejs/biome check ./src",
		"lint:write": "npx @biomejs/biome check --write",
		"test": "vitest run && pnpm test:bench",
		"test:bench": "vitest bench --run",
		"test:watch": "vitest --watch & vitest bench",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"preview": "vite preview"
	},
	"dependencies": {
		"react": "^19.2.0",
		"react-dom": "^19.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.6",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.6",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.1",
		"@vitest/coverage-v8": "4.0.10",
		"@vitest/ui": "4.0.10",
		"globals": "^16.5.0",
		"jsdom": "^27.2.0",
		"typescript": "~5.9.3",
		"vite": "npm:rolldown-vite@7.2.5",
		"vitest": "^4.0.10"
	},
	"pnpm": {
		"overrides": {
			"vite": "npm:rolldown-vite@7.2.5"
		}
	}
}
